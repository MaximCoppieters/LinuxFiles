{
   "background": {
      "scripts": [ "js/popup.js" ]
   },
   "browser_action": {
      "__default_popup": "html/popup.html",
      "default_icon": "img/icons/iconoff128.png"
   },
   "content_scripts": [ {
      "all_frames": true,
      "js": [ "js/content-script.js" ],
