   "icons": {
      "128": "img/icons/icon128.png",
      "300": "img/icons/icon300.png",
      "48": "img/icons/icon48.png"
   },
   "key": "",
   "manifest_version": 2,
   "name": "coordinates",
   "options_ui": {
      "chrome_style": true,
      "page": "html/options.html"
   },
   "permissions": [ "tabs", "http://*/*", "https://*/*", "storage" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "1.4.0"
}
